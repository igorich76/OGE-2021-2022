uses school;
{var a := ReadlnInteger('Введите число>>');
Println('Десятичная запись',a);
var b := ToBase(a,2);
Println('Двоичная запись>>',b);
var c := Dec(b, 2);
Println('Обратно>>',c);}
var d := ReadlnInteger('Введите число>>');
var e := ToBase(d,8);
Println('Восьмеричная запись>>',e);
var f := Dec(e, 8);
Println('Обратно>>',f);
